Cell[TextData[{
 "Integrate2 does integration in a Hadamard sense, i.e., ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "1"], 
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], 
     RowBox[{"\[DifferentialD]", "x"}]}], " "}], TraditionalForm]],
  ExpressionUUID->"b6ba1ba1-c2fd-45ed-a9b5-ed96be215249"],
 " means acutally expanding the result of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "\[Delta]", 
     RowBox[{"1", "-", "\[Delta]"}]], 
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], 
     RowBox[{"\[DifferentialD]", "x"}]}], " "}], TraditionalForm]],
  ExpressionUUID->"f3b2b7cc-bab6-463c-aca4-d2ba0fa37c12"],
 "up do ",
 Cell[BoxData[
  FormBox[
   RowBox[{"O", "(", "\[Delta]", ")"}], TraditionalForm]],ExpressionUUID->
  "373fceb8-a163-4670-8b7e-22eeb7ad3c9f"],
